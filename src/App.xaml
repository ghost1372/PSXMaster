<?xml version="1.0" encoding="utf-8" ?>
<Application x:Class="PSXMaster.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dev="using:DevWinUI"
             xmlns:local="using:PSXMaster">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///DevWinUI.Controls/Themes/Generic.xaml" />
                <ResourceDictionary Source="Themes/Styles.xaml" />
                <ResourceDictionary Source="Themes/ThemeResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <dev:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"
                                           FalseValue="Collapsed"
                                           TrueValue="Visible" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
